<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragment_container_view"
        android:name="androidx.navigation.fragment.NavHostFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:defaultNavHost="true"
        app:navGraph="@navigation/block_chain_nav_graph" />

    <include
        android:id="@+id/on_boarding_vertical"
        layout="@layout/widget_on_boarding_vertical_pinch"
        android:visibility="gone" />

    <include
        android:id="@+id/on_boarding_horizontal"
        layout="@layout/widget_on_boarding_horizontal_pinch"
        android:visibility="gone" />

</androidx.constraintlayout.widget.ConstraintLayout>


